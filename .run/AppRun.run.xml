    <extension name="coverage">
      <pattern>
        <option name="PATTERN" value="com.mycompany.app.*" />
        <option name="ENABLED" value="true" />
      </pattern>
    </extension>
    <method v="2">
      <option name="Make" enabled="true" />
    </method>
  </configuration>
</component>